{
  "ReverseProxy": {
    "Clusters": {
      "UserAPI": {
        "Destinations": {
          "UserAPIDestination1": {
            "Address": "http://host.docker.internal:8080/"
          }
        }
      }
    },
    "Routes": {
      "Authenticate": {
        "ClusterId": "UserAPI",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/v1/authenticate/{**catch-all}"
        }
      },
      "User": {
        "ClusterId": "UserAPI",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/v1/user/{**catch-all}"
        }
      }
    }
  }
}
